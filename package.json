    "gulp-git": "^1.7.0",
    "gulp-plumber": "^1.0.1",
    "gulp-prompt": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-s3-upload": "^1.6.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.6",
    "istanbul": "^0.4.3",
