{
  "name": "kinvey-angular2-sdk",
  "version": "3.8.1",
  "description": "Kinvey JavaScript SDK for Angular2 applications.",
  "homepage": "http://www.kinvey.com",
  "keywords": [
    "Kinvey",
    "JavaScript",
    "Angular2"
  ],
  "bugs": {
    "url": "http://support.kinvey.com"
  },
  "license": "Apache-2.0",
  "author": "Kinvey, Inc.",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "main": "kinvey-angular2-sdk.umd.js",
  "typings": "kinvey-angular2-sdk.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/angular2-sdk"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:umd && node scripts/copy-package.js && node scripts/copy-definitions.js && node scripts/copy-readme.js",
    "build:umd": "webpack --config webpack-umd.config.ts",
    "build:umd:watch": "npm run build:umd -- --watch",
    "prebuild:watch": "rimraf dist",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\"",
    "ci": "npm run lint && npm run test && npm run build",
    "clean:all": "npm run clean && rimraf node_modules",
    "clean": "rimraf dist",
    "explorer": "source-map-explorer ./dist/kinvey-angular2-sdk.umd.js",
    "lint": "npm run tslint 'src/**/*.ts'",
    "tslint": "tslint --exclude src/kinvey.d.ts",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/core": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.9",
    "@types/jasmine": "^2.5.47",
    "@types/karma": "^0.13.35",
    "@types/node": "^7.0.18",
    "@types/webpack": "^2.2.15",
    "@types/webpack-env": "^1.13.0",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "codecov": "^2.2.0",
    "codelyzer": "^3.0.1",
    "concurrently": "^3.4.0",
    "expect": "21.1.0",
    "gulp": "^3.9.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.6.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "kinvey-phonegap-sdk": "3.8.0",
    "raw-loader": "^0.5.1",
    "rimraf": "2.6.1",
    "rxjs": "^5.3.1",
    "source-map-explorer": "^1.3.3",
    "to-string-loader": "^1.1.5",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "zone.js": "^0.8.10"
  }
}
