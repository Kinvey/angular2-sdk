{
  "name": "kinvey-angular2-sdk",
  "version": "3.1.0",
  "description": "Kinvey JavaScript SDK for Angular2 applications.",
  "homepage": "http://www.kinvey.com",
  "bugs": {
    "url": "http://support.kinvey.com"
  },
  "license": "MIT",
  "author": "Kinvey, Inc.",
  "contributors": [
    "Thomas Conner <thomas@kinvey.com>"
  ],
  "main": "./src/export.js",
  "typings": "./kinvey.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:Kinvey/angular2-sdk"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run bundle && npm run minify",
    "bundle": "NODE_ENV=production webpack",
    "clean": "del coverage dist s3",
    "minify": "uglifyjs --compress warnings=false --screw-ie8 --mangle --comments --output ./dist/kinvey-angular2-sdk.min.js -- ./dist/kinvey-angular2-sdk.js ",
    "cover": "istanbul cover _mocha -- --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/setup test/unit",
    "docs": "esdoc -c esdoc.json",
    "lint": "npm run lint:src",
    "lint:src": "eslint src/**",
    "lint:test": "eslint test/unit/**",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "s3": "shjs ./scripts/before_deploy.js",
    "test": "mocha --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/setup test/unit",
    "test:watch": "mocha -w --compilers js:babel-core/register -r babel-polyfill -s 100 --recursive test/unit/setup test/unit",
    "transpile": "BABEL_ENV=production babel --no-comments --out-dir dist src",
    "version": "npm run build && git add -A dist"
  },
  "dependencies": {
    "@angular/common": "2.0.1",
    "@angular/compiler": "2.0.1",
    "@angular/core": "2.0.1",
    "@angular/http": "2.0.1",
    "@angular/platform-browser": "2.0.1",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-angular2": "0.0.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.0.15",
    "core-js": "^2.4.1",
    "del-cli": "^0.2.0",
    "expect": "^1.20.2",
    "istanbul": "^0.4.3",
    "kinvey-javascript-sdk-core": "3.2.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "reflect-metadata": "^0.1.3",
    "regenerator-runtime": "^0.9.5",
    "rxjs": "^5.0.0-beta.9",
    "webpack": "^1.13.0",
    "zone.js": "^0.6.6"
  },
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "Kinvey",
    "JavaScript",
    "Angular2"
  ]
}
